<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_weight="0.22"
            android:layout_marginTop="5sp"
            android:layout_marginBottom="5sp"
            android:background="@color/white"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/rl_listmodel"
                android:layout_width="match_parent"
                android:layout_height="100sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">


                <TextView
                    android:id="@+id/tv_s_no"
                    android:layout_marginStart="20dp"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/pheezee_text_blue_for_icon_text_background"
                    android:textSize="@dimen/Heading"
                    android:fontFamily="@font/pheezee_font"
                    style="@style/pheezee_normal"
                    android:text="12" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="10dp"
                    android:layout_toEndOf="@+id/tv_s_no"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="14 May"
                        android:textColor="@color/pheezee_text_blue_for_icon_text_background"
                        android:textSize="@dimen/Heading"
                        android:fontFamily="@font/pheezee_font"
                        style="@style/pheezee_normal"/>

                    <TextView
                        android:id="@+id/tv_exercise_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:text="11 Exercises"
                        android:textColor="@color/dark_grey_text_entry"
                        android:textSize="@dimen/subheading"
                        android:fontFamily="@font/pheezee_font"
                        style="@style/pheezee_normal"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="10dp"
                        android:gravity="bottom">

                        <TextView
                            android:id="@+id/tv_download_date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="11 Exercises"
                            android:textColor="@color/pheezee_text_blue_for_icon_text_background"
                            android:textSize="@dimen/text_entry"
                            android:fontFamily="@font/pheezee_font"
                            style="@style/pheezee_normal"
                            />
                    </LinearLayout>

                </LinearLayout>

