                <Button
                    android:id="@+id/download_icon"
                    style="@style/pheezee_normal"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:layout_alignTop="@+id/tv_s_no"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_gravity="center"
                    android:layout_marginTop="25dp"
                    android:layout_marginEnd="65dp"
                    android:layout_marginBottom="25dp"
                    android:drawableStart="@drawable/baseline_download_24"
                    android:background="@drawable/round_cancel_buttons"
                    android:focusable="false"
                    android:fontFamily="@font/pheezee_font"
                    android:text="Report"
                    android:textAllCaps="false"
                    android:textColor="@color/pheezee_text_blue_for_icon_text_background"
                    android:textSize="@dimen/mid_heading_entry"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:visibility="visible">

                </Button>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="38dp"
                    >



                    <ImageView
                        android:visibility="gone"
                        android:layout_gravity="center"
                        android:id="@+id/share_icon"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_alignTop="@+id/view_button_f"
                        android:layout_alignBottom="@+id/view_button_f"
                        android:paddingBottom="7dp"
                        android:layout_marginEnd="20dp"
                        android:src="@drawable/share_icon_n"></ImageView>

                    <ImageView
                        android:visibility="gone"
                    android:layout_gravity="center"
                    android:id="@+id/view_button"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_alignTop="@+id/view_button"
                    android:layout_alignEnd="@id/view_icon"
                    android:paddingBottom="7dp"
                    android:layout_marginEnd="20dp"
                    android:src="@drawable/download_icon_n"></ImageView>
                    <ImageView

                        android:visibility="gone"
                        android:layout_gravity="center"
                        android:id="@+id/print_icon"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_alignTop="@+id/view_button"
                        android:layout_alignEnd="@id/view_icon"
                        android:paddingBottom="7dp"
                        android:layout_marginEnd="20dp"
                        android:src="@drawable/black_print_icon"></ImageView>
                    <ImageView
                        android:visibility="gone"
                        android:layout_gravity="center"
                        android:id="@+id/print_icon_2"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_alignTop="@+id/view_button"
                        android:layout_alignEnd="@id/view_icon"
                        android:paddingBottom="7dp"
                        android:layout_marginEnd="20dp"
                        android:src="@drawable/black_print_icon"></ImageView>

                    <ImageView
                        android:layout_gravity="center"
                        android:id="@+id/view_icon"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_alignTop="@+id/view_button"
                        android:layout_alignBottom="@+id/view_button"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:src="@drawable/view_icon_n"></ImageView>



                </LinearLayout>


            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>

