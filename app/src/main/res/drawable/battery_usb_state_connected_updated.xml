<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:width="10dp" android:top="-7dp" android:gravity="center">
        <rotate
            android:fromDegrees="115"
            android:toDegrees="115"
            android:pivotX="50%"
            android:pivotY="50%" >
            <shape
                android:shape="line" >
                <stroke
                    android:width="1.5dp"
                    android:color="@color/white" />
            </shape>
        </rotate>
    </item>
    <item android:width="7dp" android:left="2dp" android:gravity="center">
            <shape android:shape="line">

                <stroke android:color="@color/white" android:width="1.5dp"/>
            </shape>
    </item>
    <item android:width="10dp" android:left="2dp" android:bottom="-7dp" android:gravity="center">
        <rotate
            android:fromDegrees="115"
            android:toDegrees="115"
            android:pivotX="50%"
            android:pivotY="50%" >
            <shape android:shape="line">
                <corners android:bottomLeftRadius="1dp" android:topLeftRadius="1dp"/>
                <stroke android:color="@color/white" android:width="1.5dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>